{
  "name": "csgo-server-api",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "dev-docker": "npm run build && npm run docker",
    "dev": "npm run babel && npm run start",
    "babel": "babel src -d build",
    "start": "node -r dotenv/config build/index.js",
    "build": "docker build -t hugojf/csgo-server-api:latest .",
    "docker": "docker run -it -p 9000:9000 -v \"$(pwd)/config:/usr/src/app/config\" hugojf/csgo-server-api:latest",
    "watch": "nodemon src/index.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sentry/node": "^5.17.0",
    "body-parser": "^1.19.0",
    "connect-timeout": "^1.9.0",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "express": "^4.17.1",
    "rcon-client": "^4.2.0",
    "request": "^2.88.2",
    "request-debug": "^0.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2"
  }
